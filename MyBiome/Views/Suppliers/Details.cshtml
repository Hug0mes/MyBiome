@model MyBiome.Models.Suppliers



<div style=" padding: 40px;
  margin-top: 40px;">

<h1>@Model.CompanyName</h1>

<dl class="row">
    <dt class="col-sm-2">Contact Name</dt>
    <dd class="col-sm-10">@Model.ContactName</dd>

    <dt class="col-sm-2">Contact Email</dt>
    <dd class="col-sm-10">@Model.ContactEmail</dd>

    <dt class="col-sm-2">Phone</dt>
    <dd class="col-sm-10">@Model.phone</dd>

    <dt class="col-sm-2">Address</dt>
    <dd class="col-sm-10">@Model.StreetAddress, @Model.City, @Model.State @Model.PostalCode</dd>
</dl>
</div>